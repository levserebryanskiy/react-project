{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "extends": ["airbnb", "react-app"],
  "rules": {
  "react/jsx-filename-extension": 0,
  "react/prefer-stateless-function": 0,
  "react/jsx-one-expression-per-line": "off",
  "linebreak-style": 0,
  "react/prop-types": "off",
  "prettier/prettier": ["error",
    {
      "endOfLine": "auto"
    }
  ]
  },
  "env": {
  "es6": true
  }
}